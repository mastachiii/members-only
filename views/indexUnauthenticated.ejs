<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head.ejs', {title: 'Members Only', css: 'indexUnauthenticated.css'}) %>
    <body>
        <div>
            <a href="/sign-up" class="signup-btn">Sign Up</a>
            <a href="/log-in" class="login-btn">Log in</a>
        </div>
        <div class="messages-container">
            <% messages.forEach(message => { %>
            <div class="message-card">
                <div>
                    <img src="<%= message.pfp_url %>" alt="profile picture" />
                    <span>
                        <p>*****</p>
                        <p>at <%= message.date %></p>
                    </span>
                </div>
                <h3><%= message.title %></h3>
                <p><%= message.message %></p>
            </div>
            <% }) %>
        </div>
    </body>
</html>
